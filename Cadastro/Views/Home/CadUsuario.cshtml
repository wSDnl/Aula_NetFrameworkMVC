<!-- Basic layout-->

<div class="panel panel-flat">

    <div class="panel-body">

        <div class="form-group">
            <label>Nome: </label>
            <input id="txtNomeCad" type="text" class="form-control" style="text-transform:capitalize">
        </div>

        <div class="form-group">
            <label>E-mail: <label class="avisoRedEmail">Formato de e-mail inválido</label> </label>
            <input id="txtEmailCad" type="text" class="form-control" style="text-transform:lowercase">
        </div>

        <div class="form-group">
            <label>Senha:</label>
            <input id="txtSenhaCad" type="password" class="form-control">
        </div>

        <div class="text-right">
            <input type="button" id="btnSalvarCad"  value="SALVAR" class="btn btn-primary" />
        </div>

    </div>
</div>
<!-- /basic layout -->